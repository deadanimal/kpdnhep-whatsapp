{
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lomotech/driver-telegram"
        }
    ],
    "license": "MIT",
    "name": "laravel/laravel",
    "require": {
        "php": ">=5.6.4",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-imap": "*",
        "ext-json": "*",
        "botman/botman": "^2.5",
        "box/spout": "*",
        "doctrine/dbal": "2.5.*",
        "facebook/graph-sdk": "^5.6",
        "graham-campbell/exceptions": "^10.0",
        "guzzlehttp/oauth-subscriber": "^0.3.0",
        "intervention/image": "*",
        "kitetail/zttp": "^0.6.0",
        "kryptonit3/counter": "5.2.*",
        "lab404/laravel-impersonate": "~1.1.0",
        "laracasts/utilities": "^3.0",
        "laravel/framework": "5.4.*",
        "laravel/socialite": "^3.2",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "5.4.*",
        "lomotech/driver-telegram": "^1.5",
        "maatwebsite/excel": "^2.1",
        "mews/captcha": "^2.2",
        "mpdf/mpdf": "*",
        "nexmo/client": "*",
        "niklasravnsborg/laravel-pdf": "1.5.0",
        "orangehill/iseed": "^2.6",
        "php-imap/php-imap": "^4.1",
        "phpoffice/phpword": "^0.14.0",
        "predis/predis": "^1.1",
        "socialiteproviders/instagram": "^3.0",
        "spatie/laravel-backup": "^3.0.0",
        "webklex/laravel-imap": "^1.2",
        "xethron/migrations-generator": "*",
        "yajra/laravel-datatables-oracle": "8.3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.4",
        "filp/whoops": "^2.1",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]    
    },
    "type": "project"
}
